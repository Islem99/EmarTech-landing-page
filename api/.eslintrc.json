{
    "env": {
        "commonjs": true,
        "es2021": true
    },
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
       "require-jsdoc": "off",
       "indent": ["error", 2], // Set 2 space indentation
       "object-curly-spacing": ["error", "always"],
       "quotes": ["error", "double"],
       "max-len": ["error", { "code": 120 }]
    }
}
